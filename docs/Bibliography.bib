@article{SIN09,
	Abstract = {Textile fibers are characterized by many parameters and used by both researchers and industrial users. Many studies focus on the effect of fiber length and fineness and maturity (only for cotton) and tensile properties and etc. on the properties of intermediate products and yarns. Contrary and few studies concerning the fiber surface properties and especially the inter-fiber friction and appear in the scientific literature. A newand simple device is designed and used in this. It enables the measurement of inter-fiber friction force during the dissociation of an ordinary sliver. Contrary to what has previously been mentioned in the scientific literature and the study of a set of different cottons shows that the inter-fiber friction seems to obey the Amontons{\'\i} law rather than a more complex power law model. The calculated inter-fiber friction coefficient helps to differentiate the different types of cotton and to bring to the fore the non-negligible effect of the friction on yarn irregularity and imperfections and affecting the yarn tenacity indirectly. Meanwhile and this work has revealed a curious grouping of different types of cotton and which is not observed before. This phenomenon is undoubtedly due to the surface properties of fibers.},
	Author = {A. Sinoimeri},
	Journal = {Wear},
	Owner = {cflorimond},
	Pages = {1619-1624},
	Timestamp = {2011.03.22},
	Title = {Friction in textile fibers and its role in fiber processing},
	Volume = {267},
