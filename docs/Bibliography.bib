@article{DAS96,
	Abstract = {This paper presents effective thermo-mechanical and thermal properties of plain-weave fabric-reinforced composite laminates obtained from micromechanical analyses and a two-scale asymptotic homogenization theory. A unit cell, enclosing the characteristic periodic repeat pattern in the fabric weave, is isolated and modeled. The orthotropic tensors for effective mechanical stiffness, coefficient of thermal expansion and thermal conductivity are obtained by numerically solving appropriate microscale boundary value problems (BVPs) in the unit cell by the use of three-dimensional finite element analyses. Further, analytical models consisting of series-parallel thermal resistance networks are developed in order, to obtain orthotropic thermal conductivity. The numerical and analytical models are explicitly based on the properties of the constituent materials and three-dimensional features of the weave style. Results obtained from the models are compared with experimental values and with models available in the literature. Non-linear mechanical constitutive behavior due to resin stress/strain non-linearity and to transverse yarn damage under in-plane uni-axial loads are also investigated. Parametric studies are conducted to examine the effect of varying fiber volume fractions on the effective thermal properties.},
	Author = {Dasgupta, a. and Agarwal, R. K. and Bhandarkar, S. M.},
	Doi = {10.1016/0266-3538(95)00111-5},
	File = {:C$\backslash$:/Users/eguzmanmal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dasgupta, Agarwal, Bhandarkar - 1996 - Three-dimensional modeling of woven-fabric composites for effective thermo-mechanical and thermal.pdf:pdf},
	Isbn = {0266-3538},
	Issn = {02663538},
	Journal = {Composites Science and Technology},
	Keywords = {Analytical model,Coefficient of thermal expansion,Effective properties,Fabric composite,Finite element analysis,Homogenization,Plain-weave,Stiffness,Thermal conductivity},
	Number = {95},
	Owner = {eguzmanmal},
	Pages = {209--223},
	Timestamp = {2015.09.11},
	Title = {Three-dimensional modeling of woven-fabric composites for effective thermo-mechanical and thermal properties},
	Volume = {56},
	Year = {1996},
	Bdsk-Url-1 = {https://doi.org/10.1016/0266-3538(95)00111-5}}

@InCollection{DAV12,
  author        = {Geoffrey Davies},
  booktitle     = {Materials for Automobile Bodies},
  title         = {Chapter 3 - Materials for consideration and use in automotive body structures},
  doi           = {http://dx.doi.org/10.1016/B978-0-08-096979-4.00003-7},
  editor        = {Davies, Geoffrey},
  isbn          = {978-0-08-096979-4},
  pages         = {93 - 143},
  publisher     = {Butterworth-Heinemann},
  url           = {http://www.sciencedirect.com/science/article/pii/B9780080969794000037},
  abstract      = {Materials for consideration and use in automotive body structures The wide range of materials that may be considered for the construction of the automobile body is first evaluated with respect to their relevant mechanical and physical properties. Then, the rationale for the selection of key materials, referred to in Chapter 2, is explored, extending the list of necessary attributes to include those relating to production and service. For each of these materials, a quantitative assessment for each criterion, with relative ratings, is given. Not only are the mechanical properties compared, important from an engineering viewpoint, but ratings are also given for cost, manufacturability, recyclability and reparability. Allowance must be made for the category of vehicle being considered, e.g. volume, prestige or performance model. Overall this approach provides a useful comparison not presented previously. Importantly, the production processes relevant to the key materials are covered, giving an insight into the versatility and limitations of each, and providing useful background knowledge for issues relating to suppliers. Detailed information is given on the latest grades of the ever-growing family of high-strength steels, together with the aluminum alloys, magnesium and composites now in use. As well as inherent properties, factors such as surface technology, affecting both processing and cosmetic finish, are considered. Reparability is another attribute that assumes greater importance as complexity of materials and hybridization of body panel combinations, as well as insurance cost, increase. This is now accorded a separate section.},
