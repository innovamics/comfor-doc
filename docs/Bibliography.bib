@article{CHE10,
	Abstract = {This article addresses the requirement to generate solid hexahedral finite element meshes for modelling composite components with complex geometric shapes. Standard commercial finite element pre-processors are not capable of unambiguously defining the material orientations in such situations. This has necessitated the development of a purpose written pre-processor and which extrudes solid elements with element orientations that accurately match complex doubly curved geometries. An efficient methodology for the creation of the mesh has been outlined through the combined use of the dedicated software and MSC PATRAN. An example presented shows that this technology has been successfully used to model a representative vane from a gas turbine engine.},
	Author = {J. Chen and S. Hallet and M. R. Wisnom},
	Journal = {Computers \& Structures},
	Number = {9-10},
	Owner = {cflorimond},
	Pages = {602-609},
	Timestamp = {2011.03.22},
	Title = {Modelling complex geometry using solid finite element meshes with correct composite material orientations},
	Volume = {88},
	Year = {2010}}

@article{CHE99a,
	Abstract = {This paper describes a study of the microstructure of 3D braided preforms produced by the four-step 1*1 method. An analytical approach is employed in conjunction with experimental investigation to establish the relationship between the braid structure and braiding parameters. Microscopic evidence of the microstructure of preforms reveals the dfferent configurations of the yarns in the interior and surface and corner regions of a braided preform. On the basis of microscopic observations and three types of microstructural unit cell models are established for the three regions. By using these three unit cell models and the structural geometry of the preforms is analyzed and the mathematical relationships among the structural parameters and such as the yarn packing factor and fiber orientation and fiber volume fraction and braiding pitch and are derived. From the preforms to the final composites and if the RTM process is utilized and the unit cell size and shape will be changed. The modification of the relationships among the structural parameters has been made. Good agreement has been obtained between the calculated and measured values of the geometric characteristics of braided composite samples.},
	Author = {L. Chen and X. M. Tao and C. L. Choy},
	Journal = {Composites Science and Technology},
	Owner = {cflorimond},
	Pages = {391-404},
	Timestamp = {2011.03.22},
	Title = {On the microstructure of three-dimensional braided preforms},
	Volume = {59},
	Year = {1999}}

