@article{HA-11,
	Abstract = {In this study and a FEM analysis has been carried out to find out pertinent multi-scale model for an investigation of a ballistic impact on 2D KM2 plain-woven fabrics. Multi-scale models are a combination between macroscopic and mesoscopic models. This study aims at testing a multi-scale model in order to minimize the computing time. Three configurations were analyzed by varying the ratio of macroscopic and mesoscopic areas: 75.3{\~n}24.7% and 65.5{\~n}34.5% and 56.3{\~n}43.7% with two impact velocities 60 m/s and 245 m/s. In these multi-scale models and the continuity in macroscopic{\~n}mesoscopic interfaces is ensured by checking the evolution of global displacements of the fabric during impact. The effect of the macroscopic area of multi-scale models on the ballistic performance of the fabric is also investigated. The optimal multi-scale model was validated by comparison with results obtained from a mesoscopic model in terms of the evolutions of the projectile velocity and energy forms and the overall behavior of the fabric during impact and the force applied on the projectile. The failure criterion Forming Limited Diagram (FLD) is suggested for bundle failure. The observed damage mechanisms of the fabric during penetration time of the projectile are discussed and compared among numerical models.},
	Author = {C. Ha-Minh and T. Kanit and F. Boussu and A. Imad},
	Journal = {Computational Materials Science},
	Number = {7},
	Owner = {cflorimond},
	Pages = {2172-2184},
	Timestamp = {2011.03.25},
	Title = {Numerical multi-scale modeling for textile woven fabric against ballistic impact},
	Volume = {50},
	Year = {2011}}

