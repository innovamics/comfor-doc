@article{LOM08,
	Abstract = {Full-field strain measurements are applied in studies of textile deformability during composite processing: (1) in testing of shear and tensile deformations of textiles (picture frame and bias and biaxial extension test) as an {\"e}{\"e}optical extensometer{\'\i}{\'\i} and allowing accurate assessment of the sample deformation and which may differ significantly from the deformation applied by the testing device; (2) to study mechanisms of the textile deformation on the scale of the textile unit cell and of the individual yarns (meso- and micro-scale full-field strain measurements); (3) to measure the 3D-deformed shape and the distribution of local deformations (e.g. and shear angles) of a textile reinforcement after draping and providing input data for the validation of material drape models and for the prediction of the consolidated part performance via structural finite element analysis. This paper discusses these three applications of the full-field strain measurements and providing examples of studies of deformability of woven (glass and glass/PP) and non-crimp (carbon) textile reinforcements. The authors conclude that optical full-field strain techniques are the preferable (sometimes the only) way of assuring correct deformation measurements during tensile or shear tests of textile.},
	Author = {S. V. Lomov and P. Boisse and E. De Luycker and F. Morestin and K. Vanclooster and D. Vandepitte and I. Verpoest and A. Willems},
	Journal = {Composites Part A: Applied Science and Manufacturing},
	Number = {8},
	Owner = {cflorimond},
	Pages = {1232-1244},
	Timestamp = {2011.03.22},
	Title = {Full-field strain measurements in textile deformability studies},
	Volume = {39},
	Year = {2008}}

@Article{LOM11e,
  author        = {Stepan V. Lomov and Larissa Gorbatikh and Matthieu Houlle and ≈Ωeljko Kotanjac and Vitaly Koissin and Katleen Vallons and Ignaas Verpoest},
  title         = {Compression resistance and hysteresis of carbon fibre tows with grown carbon nanotubes/nanofibres},
  issn          = {0266-3538},
  number        = {15},
  pages         = {1746 - 1753},
  volume        = {71},
  abstract      = {Growth of carbon nanotubes (CNT) or carbon nano-fibres (CNF) on fibrous substrates is a way to increase the fracture toughness of fibre reinforced composites (FRC), with encouraging results reported in the recent years. The issues for these materials related to manufacturing of these composites are, however, less investigated. Following the study of compressibility of woven carbon fibre preforms with CNT/CNFs grown on the fibres using the \{CVD\} method [Compos Sci Technol 2011; 71(3): 315‚{\"A}{\`\i}325], this paper describes compression tests on the carbon tows used in these fabrics. The results of the measurements include pressure vs. thickness diagrams in consecutive compression cycles and hysteresis of the compression. The results confirm a drastic change of compressibility of fibrous assemblies in the presence of CNT/CNF grafting.},
  journal       = {Composites Science and Technology},
  keywords      = {A. Nano composites},
  owner         = {nnaouar},
  timestamp     = {2013.11.03},
  year          = {2011},
}

@article{LOM00,
	Abstract = {A mathematical model of the internal geometry of 2D and 3D woven fabrics is used as a unit-cell geometry preprocessor for meso-mechanical models of composite materials. The model computes a spatial placement of all yarns in a fabric repeat for a given weave structure (a special coding algorithm is employed) and given warp and weft yarns geometrical and mechanical parameters. The source of these data is the manufacturer's documentation for a woven fabric and yarn mechanical behaviour in bending and compression and measured on standard textile-laboratory equipment. The principle of minimum energy of a fibrous assembly leads to the prediction of the yarns crimp and the forces in the warp/weft contacts and the yarn compression within a fabric. The input data for use in meso-mechanical models of composite materials is therefore provided in addition to tools for the 3D representation of the woven structure and for the creation of the unit-cell cross-sections and for the computation of the unit cell porosity},
	Author = {S. V. Lomov and A. V. Gusakov and G. Huysmans and A. Prodromou and I. Verpoest},
	Journal = {Composites Science and Technology},
	Number = {11},
	Owner = {cflorimond},
	Pages = {2083-2095},
	Timestamp = {2011.03.22},
	Title = {Textile geometry preprocessor for meso-mechanical models of woven composites},
	Volume = {60},
	Year = {2000}}

