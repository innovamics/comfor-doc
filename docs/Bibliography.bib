@Article{MEY09,
  author        = {B.C. Meyer and Ch.V. Katsiropoulos and Sp.G. Pantelakis},
  title         = {Hot forming behavior of non-crimp fabric peek/c thermoplastic composites},
  issn          = {0263-8223},
  number        = {2},
  pages         = {225 - 232},
  volume        = {90},
  abstract      = {The hot forming behavior of four new non-crimp fabric (NCF) composite materials developed by using the thermoplastic resin PolyEtherEtherKetone (PEEK) was experimentally investigated. The quality of the hot formed parts was evaluated by means of C-Scan and optical microscopy analyses. In addition, the draping behavior of the \{NCFs\} was experimentally evaluated to assess the suitability of the developed \{NCF\} materials for being exploited in composite component manufacturing technologies involving direct lay-up processes. A complete series of tests, including hot forming tests, quality evaluation of the produced parts and drapeability tests, was also carried out for the thermoplastic composite APC-2/AS4 (PEEK reinforced with carbon fibers) to obtain a reference for comparison. The comparison has shown that the hot forming process may be used to shape simple parts from thermoplastic \{NCF\} composites providing an acceptable quality at a reasonable cost. Furthermore, the improved drapeability of the \{NCF\} material allows for a direct lay-up in a curved form tool. Finally, an assessment of the potential of the developed \{NCF\} composites for use in aircraft structural applications was made.},
  journal       = {Composite Structures},
  keywords      = {Fabrics/textiles},
  owner         = {nnaouar},
  timestamp     = {2013.11.03},
  year          = {2009},
}

@article{MIA08,
	Abstract = {Textile fabric geometry determines textile composite properties. Textile process mechanics determines fabric geometry. In previous papers and the authors proposed a digital element model to generate textile composite geometry by simulating the textile process. The greatest difficulty encountered with its employment in engineering practice is efficiency. A full scale fiber-based digital element analysis would consume huge computational resources. Two advances are developed in this paper to overcome the problem of efficiency. An improved contact-element formulation is developed first. The new formulation improves accuracy. As such and it permits a coarse digital element mesh. Then and a static relaxation algorithm to determine fabric micro-geometry is established to replace step-by-step textile process simulation. Employing the modified contact element formulation in the static relaxation approach and the required computer resource is only 1{\~n}2% of the resource required by the original process. Two critical issues with regards to the digital element mesh are also examined: yarn discretization and initial yarn cross-section shape. Fabric geometries derived from digital element analysis are compared to experimental results.},
	Author = {Y. Miao and E. Zhou and Y. Wang and B. A. Cheeseman},
	Journal = {Composites Science and Technology},
	Number = {7-8},
	Owner = {cflorimond},
	Pages = {1671-1678},
	Timestamp = {2011.03.22},
	Title = {Mechanics of textile composites: Micro-geometry},
	Volume = {68},
	Year = {2008}}

@article{MIC11,
	Abstract = {The mechanical behaviour of cellular structures is governed by their internal architecture. Therefore, it is of interest to incorporate a quantitative description of actual three-dimensional microstructures in the mechanical analysis of materials. In this contribution, an experimental-computational methodology has been developed to facilitate finite element method simulations of complex three-dimensional cellular structures, i.e. to open-cell Ni and Al foams. Two different approaches were examined to visualize the exact three-dimensional (3D) structure of the examined Ni and Al foams, X-ray computed tomography and a serial sectioning image-based process, respectively. In situ micro-tensile tests were carried out in order to determine the foam mechanical properties while scanning electron microscopy was used to monitor the deformation and fracture of the struts. Both experimental results and FEM simulations reveal that the distributions of local stresses and strains depend on size, orientation, and spatial arrangement of the pores.},
	Author = {Michailidis, N. and Stergioudi, F. and Omar, H. and Papadopoulos, D. and Tsipas, D.N.},
	Booktitle = {A collection of papers from the 8th EUFOAM Conference and the Meetings of COST Actions D43 and P21},
	Issn = {0927-7757},
	Journal = {Colloids and Surfaces A: Physicochemical and Engineering Aspects},
	Keywords = {Metal foam, Open cell, Micro-tension, X-ray tomography, FEM simulation},
