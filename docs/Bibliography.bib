@inproceedings{FOU04,
	Abstract = {Skeletons are compact representations that allow mathematical analysis of objects. A skeleton must be homotopic, thin and medial in relation to the object it represents. Numerous approaches already exist which focus on computational efficiency. However, when dealing with data too large to be loaded into the main memory of a personal computer, such approaches can no longer be used. We present in this article a skeletonization algorithm that processes the data locally (in sub-images) while preserving global properties (medial localization). Our privileged application is the study of the cerebral micro-vascularisation, and we show some results obtained on a mosaic of 3-D images acquired by confocal microscopy.},
	Author = {Fouard, C. and Cassot, E. and Malandain, G. and Mazel, C. and Prohaska, S. and Asselot, D. and Westerhoff, M. and Marc-Vergnes, J.P.},
	Booktitle = {Biomedical Imaging: Nano to Macro, 2004. IEEE International Symposium on},
	Keywords = {Computational efficiency, Europe, Image analysis, Mathematical analysis, Microcomputers, Microscopy, Petroleum, Shape, Skeleton, Surface morphology, biomedical optical imaging, brain, haemorheology, image thinning, medical image processing, optical microscopy, 3-D images, brain microcirculation, cerebral microvascularisation, confocal microscopy, image skeletonization, large 3D datasets, mathematical analysis, medial localization, object it represents},
	Owner = {nnaouar},
